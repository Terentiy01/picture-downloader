-------\r\n\r\nexport function upload(selector, fotki) {\r\n  const input = document.querySelector(selector)\r\n  const preview = document.createElement('div')\r\n\r\n  preview.classList.add('preview')\r\n\r\n  if (fotki.multi) {\r\n    input.setAttribute('multiple', true)\r\n  }\r\n\r\n  if (fotki.accept) {\r\n    input.setAttribute('accept', fotki.accept.join(','))\r\n  }\r\n\r\n  const open = document.createElement('button')\r\n  open.classList.add('btn')\r\n  open.textContent = 'Открыть'\r\n\r\n  const klik = () => input.click()\r\n\r\n  const changer = (event) => {\r\n    if (!event.target.files.length) {\r\n      return\r\n    }\r\n\r\n    const files = Array.from(event.target.files)\r\n\r\n    preview.innerHTML = ''\r\n\r\n    files.forEach((f) => {\r\n      if (!f.type.match('image')) {\r\n        return\r\n      }\r\n      const reader = new FileReader()\r\n\r\n      reader.onload = (event) => {\r\n        const src = event.target.result\r\n        preview.insertAdjacentHTML(\r\n          'afterbegin',\r\n          `<div class=\"preview-image\">\r\n          <div class=\"preview-remove\">&times;</div>\r\n          <img src=\"${src}\" alt=\"${f.name}\"/>\r\n          <div class=\"preview-info\">\r\n          <span>${f.name}</span>\r\n          <span>${f.size}</span>\r\n          </div>\r\n          </div>\r\n          `\r\n        )\r\n      }\r\n\r\n      reader.readAsDataURL(f)\r\n    })\r\n  }\r\n\r\n  input.insertAdjacentElement('afterend', preview)\r\n  input.insertAdjacentElement('afterend', open)\r\n  input.addEventListener('change', changer)\r\n  open.addEventListener('click', klik)\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"266c76da5d021f564a8202a20ed4aca1","cacheData":{"env":{}}}